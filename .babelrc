{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@api": "./src/api",
          "@assets": "./assets",
          "@store": "./src/store",
          "@models": "./src/models",
          "@components": "./src/components",
          "@navigation": "./src/navigation",
          "@screens": "./src/screens",
          "@utils": "./src/utils"
        }
      }
    ],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["inline-dotenv"]
  ]
}
